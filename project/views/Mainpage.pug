    line-height: 1.375em;
    color: #303030;
    font-weight: 400;
    margin-bottom: 30px;
    }
    .jumbotron {
    background-color: #000000;
    color: #fff;
    padding: 327px 25px;
    font-family: Montserrat, sans-serif;
    }
    .container-fluid {
    padding: 60px 50px;
    }
    .bg-grey {
    background-color: #f6f6f6;
    }
    .logo {
    color: #000000;
    font-size: 200px;
    }
    .thumbnail {
    padding: 0 0 15px 0;
    border: none;
    border-radius: 0;
    }
    .thumbnail img {
    width: 100%;
    height: 100%;
    margin-bottom: 10px;
    }
    .carousel-control.right,
    .carousel-control.left {
    background-image: none;
    color: #000000;
    }
    .carousel-indicators li {
    border-color: #000000;
    }
    .carousel-indicators li.active {
    background-color: #000000;
    }
    .item h4 {
    font-size: 19px;
    line-height: 1.375em;
    font-weight: 400;
    font-style: italic;
    margin: 70px 0;
    }
    .item span {
    font-style: normal;
    }
    .panel {
    border: 1px solid #000000;
    border-radius: 0 !important;
    transition: box-shadow 0.5s;
    }
    .panel:hover {
