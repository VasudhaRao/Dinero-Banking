    }
    .fa-instagram {
    background: #cd486b;
    color: white;
    }
    .fa-youtube {
    background: red;
    color: white;
    }
    .carousel-item {
    height: 100vh;
    min-height: 350px;
    background: no-repeat center center scroll;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    }
    section {
