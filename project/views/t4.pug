doctype html
head
  title userdash
  meta(name='viewport' content='width=device-width, initial-scale=1')
  link(rel='stylesheet' href='https://www.w3schools.com/w3css/4/w3.css')
  link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css')
  script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
  script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js')
  style.
    body {
    font: 400 15px Lato, sans-serif;
    line-height: 1.8;
    color: #818181;
    }
    .navbar {
    margin-bottom: 0;
    background-color: #000000;
    z-index: 9999;
    border: 0;
    font-size: 12px !important;
    line-height: 1.42857143 !important;
    letter-spacing: 4px;
    border-radius: 0;
    font-family: Montserrat, sans-serif;
    }
    .navbar li a,
    .navbar .navbar-brand {
    color: white !important;
    }
    .navbar-nav li a:hover,
    .navbar-nav li.active a {
    color: black !important;
    background-color: white !important;
    }
    .navbar-default .navbar-toggle {
    border-color: transparent;
    color: #fff !important;
    }
    body{
    margin: 0;
    padding: 0;
    background: url(https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQx3wOeePAytt8XcXntcSpqaLBDULud7mzM_DallHpJ3AvdGywZ);
    background-size: cover;
    background-position: center;
    font-family: sans-serif;
    }
    body,
    html {
    height: 100%
    }
    body,
    h1,
    h2,
    h3,
    h4,
    h5,
    h6 {
    font-family: "Amatic SC", sans-serif
    }
    .menu {
    display: none
    }
.w3-bar.w3-black
  a.w3-bar.w3-black(onclick="openreq('gas')")
    img(src='data:image/png;base64,' alt='logo' style='width:3%;')
  button.w3-bar-item.w3-button.w3-animate-right(onclick="openreq('gas')") GAS
  button.w3-bar-item.w3-button.w3-animate-right(onclick="openreq('electricity')") ELECTRICITY
  button.w3-bar-item.w3-button.w3-animate-right(onclick="openreq('mobile')") MOBILE
#gas.w3-container.req
  .container
    br
    br
    br
    h3 #{message}
    form.form-horizontal(action='/t6/gas' method='post')
      .form-group
       
        label.control-label.col-sm-4(for='billno') Bill No : 
        .col-sm-5
          input#billno.form-control(type='text' placeholder='Bill no' name='billnum' pattern='[0-9]{6}$' required='')
      br
      .form-group
        label.control-label.col-sm-4(for='amt') Company : 
        .col-sm-5
          select(name='cname')
            option select
            option INDANE
            option HP
      br
      .form-group
        label.control-label.col-sm-4(for='mpin') MPIN : 
        .col-sm-5
          input#mpin.form-control(type='password' placeholder='mpin' name='mpin' pattern='[0-9]{4}$' required='')
      br
      .form-group
        
        .col-sm-offset-5.col-sm-5
          button.btn.btn-default(type='submit') PAY
#electricity.w3-container.req(style='display:none;')
  .container
    br
    br
    br
    h3 #{message}
    form.form-horizontal(action='/t6/electricity' method='post')
      .form-group
        label.control-label.col-sm-4(for='billno') Bill No : 
        .col-sm-5
          input#billno.form-control(type='text' placeholder='Bill no' name='billnum' pattern='[0-9]{9}$' required='')
      br
      .form-group
        label.control-label.col-sm-4(for='amt') State : 
        .col-sm-5
          select(name='cname')
            option select
            option ANDHRA PRADESH
            option TAMIL NADU
      br
      .form-group
        label.control-label.col-sm-4(for='mpin') MPIN : 
        .col-sm-5
          input#mpin.form-control(type='password' placeholder='mpin' name='mpin' pattern='[0-9]{4}$' required='')
      br
      .form-group
        
        .col-sm-offset-5.col-sm-5
          button.btn.btn-default(type='submit') PAY
#mobile.w3-container.req(style='display:none;')
  .container
    br
    br
    br
    h3 #{message}
    form.form-horizontal(action='/t6/mobile' method='post')
      .form-group
        label.control-label.col-sm-4(for='mobno') Mobile No : 
        .col-sm-5
          input#mobno.form-control(type='text' placeholder='Mobile no' name='mobile' pattern='[0-9]{10}$' required='')
      br
      .form-group
        label.control-label.col-sm-4(for='amt') Company : 
        .col-sm-5
          select(name='cname')
            option select
            option JIO
            option BSNL
            option AIRTEL
      br
      .form-group
        label.control-label.col-sm-4(for='amt') MPIN :
        .col-sm-5
          input#amt.form-control(type='password' placeholder='mpin' name='mpin' pattern='[0-9]{4}$' required='')
      br
      .form-group
        
        .col-sm-offset-5.col-sm-5
          button.btn.btn-default(type='submit') PAY
script.
  function openreq(cityName) {
  var i;
  var x = document.getElementsByClassName("req");
  for (i = 0; i < x.length; i++) {
  x[i].style.display = "none";
  }
  document.getElementById(cityName).style.display = "block";
  }
  function openRightMenu() {
  document.getElementById("rightMenu").style.display = "block";
  }
  function closeRightMenu() {
  document.getElementById("rightMenu").style.display = "none";
  }
  function openCity(cityName) {
  var i;
  var x = document.getElementsByClassName("city");
  for (i = 0; i < x.length; i++) {
  x[i].style.display = "none";
  }
  document.getElementById(cityName).style.display = "block";
  }